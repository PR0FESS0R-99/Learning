<style>
    @import url(/stylesheets/admins/navbar.css);
</style>

<nav class="navbar">
    <div class="title">
        <a href="/admin/dashboard">BOOK STORE</a>
    </div>

    {{#if isLoggedIn}}
    <i class='bx bx-log-in-circle' onclick="logout()" linkButton('/admin/dashboard')"" id="logout"></i>
    {{/if}}
</nav>

<script>
    function logout() {
        $.ajax({
            url: '/log-out',
            type: 'GET',
            success: function (data) {
                window.location.reload();
            }
        })
    }
</script>